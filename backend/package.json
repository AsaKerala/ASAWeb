{
  "name": "organization-website-backend",
  "version": "1.0.0",
  "description": "Backend for organization website built with Payload CMS",
  "main": "dist/server.js",
  "scripts": {
    "dev": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts nodemon",
    "build:payload": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload build",
    "build:server": "tsc",
    "build": "npm run build:payload && npm run build:server",
    "serve": "cross-env PAYLOAD_CONFIG_PATH=dist/payload.config.js NODE_ENV=production node dist/server.js",
    "seed": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts ts-node src/seed/index.ts",
    "generate:types": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload generate:types",
    "generate:graphQL": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload generate:graphQLSchema",
    "docker:build": "docker build -t asaweb-backend .",
    "docker:dev": "cd .. && docker-compose -f docker-compose.dev.yml up -d backend",
    "debug": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts NODE_ENV=development DEBUG=* ts-node src/server.ts"
  },
  "keywords": [
    "payload-cms",
    "cms",
    "organization",
    "mongodb",
    "express"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@payloadcms/bundler-webpack": "^1.0.5",
    "@payloadcms/db-mongodb": "^1.0.8",
    "@payloadcms/richtext-slate": "^1.2.0",
    "@types/bcrypt": "^5.0.2",
    "@types/multer": "^1.4.12",
    "bcrypt": "^5.1.1",
    "cloudinary": "^1.41.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "multer": "^1.4.5-lts.2",
    "multer-storage-cloudinary": "^4.0.0",
    "payload": "^2.4.0",
    "payload-cloudinary": "github:SyedMuzamilM/payload-cloudinary",
    "payload-cloudinary-plugin": "^0.1.14",
    "twilio": "^5.5.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.17",
    "@types/node": "^20.17.24",
    "assert": "^2.1.0",
    "browserify-zlib": "^0.2.0",
    "buffer": "^6.0.3",
    "constants-browserify": "^1.0.0",
    "cross-env": "^7.0.3",
    "crypto-browserify": "^3.12.1",
    "https-browserify": "^1.0.0",
    "nodemon": "^3.0.1",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "url": "^0.11.4",
    "util": "^0.12.5",
    "webpack": "^5.99.6",
    "webpack-cli": "^6.0.1",
    "webpack-node-externals": "^3.0.0"
  }
}
